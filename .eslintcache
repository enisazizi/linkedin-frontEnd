 const res = await fetch(`${process.env.REACT_APP_API_URL}/profiles/me`, {\r\n  //       headers: {\r\n  //         Authorization: \"Basic \" + localStorage.getItem(\"token\"),\r\n  //       },\r\n  //     });\r\n  //     const user = await res.json();\r\n  //     console.log(\"login user\", user);\r\n  //     this.setState({ me: user });\r\n  //     localStorage.setItem(\"id\", user._id);\r\n  //   } catch (error) {\r\n  //     console.log(error);\r\n  //   }\r\n  // };\r\n  componentDidMount() {\r\n    this.getPosts();\r\n    // this.getUser();\r\n  }\r\n  toggleModal = (data) => {\r\n    console.log(\"toggle data\", data);\r\n    if (data === undefined) this.setState({ post: {} });\r\n    this.setState({ showModal: !this.state.showModal });\r\n    this.setState({ post: data });\r\n  };\r\n  render() {\r\n    const {\r\n      err,\r\n      loading,\r\n      posts,\r\n      me,\r\n      errMsg,\r\n      links,\r\n      morePosts,\r\n      showModal,\r\n      post,\r\n    } = this.state;\r\n    return (\r\n      <div className=\"homeDiv\">\r\n        {err && (\r\n          <Alert variant=\"danger\" className=\"HomeCont\">\r\n            {errMsg}\r\n          </Alert>\r\n        )}\r\n        <Container className=\"HomeCont justify-content-center d-flex w-100\">\r\n          {loading && err !== true ? (\r\n            <FadeLoader loading={loading} size={60} />\r\n          ) : (\r\n            <Row>\r\n              <Col className=\"d-none d-lg-block\" md={3}>\r\n                <RSidebar me={this.props.me} />\r\n              </Col>\r\n              <Col lg={6} md={9}>\r\n                <PostModal\r\n                  me={this.props.me}\r\n                  refetch={() => this.getPosts(true)}\r\n                />\r\n                <Row className=\"sortBySeperator\">\r\n                  <hr className=\"hLine\" />\r\n                  <span>Sort by recent</span>\r\n                </Row>\r\n                {posts.map((post) => (\r\n                  <FeedCard\r\n                    key={uniqid()}\r\n                    post={post}\r\n                    toggle={() => this.toggleModal(post)}\r\n                  />\r\n                ))}\r\n                {morePosts && (\r\n                  <Button\r\n                    variant=\"outline-secondary\"\r\n                    onClick={() => this.getPosts(false, links.next)}\r\n                    className=\"w-100\"\r\n                  >\r\n                    More\r\n                  </Button>\r\n                )}\r\n              </Col>\r\n              <Col className=\"d-none d-md-block\" md={3}>\r\n                <Sidebar />\r\n              </Col>\r\n            </Row>\r\n          )}\r\n        </Container>\r\n        {showModal && (\r\n          <EditPost\r\n            me={this.props.me}\r\n            show={showModal}\r\n            post={post}\r\n            toggle={() => this.toggleModal()}\r\n            refetch={() => this.getPosts(true)}\r\n          />\r\n        )}\r\n      </div>\r\n    );\r\n  }\r\n}\r\n","C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\pages\\Welcome\\index.jsx",[],["91","92"],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\components\\AppNavBar\\index.jsx",["93","94"],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\components\\BioCard\\index.jsx",[],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\components\\Experience\\index.jsx",[],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\components\\SearchResult\\index.jsx",[],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\components\\EditExp\\index.jsx",[],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\index.js",[],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\pages\\Signup\\index.jsx",[],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\pages\\Footer\\index.jsx",[],"C:\\Users\\enisa\\documents\\github\\bw3-linkedin-frontend\\src\\c